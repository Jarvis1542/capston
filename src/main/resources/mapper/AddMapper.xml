<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fivekm_home.charge.mapper.AddMapper">

    <select id="scsList" resultType="com.fivekm_home.charge.domain.USER.SCSList">
        select BASICPAY, OVERPAY, DATE_BOOKSTART, DATE_BOOKEND, ROUND((DATE_BOOKEND - DATE_BOOKSTART)*24*60) as BOOK_RESULT from SCS
     </select>
    <select id="graphList" resultType="com.fivekm_home.charge.domain.USER.GraphList">
        select CREATE_DATE from member_view
     </select>

<!--    그 날짜에 해당하는 월요일로 출력 -->
    <select id="weekList" resultType="com.fivekm_home.charge.domain.ADDITION.WEEKLISTDTO">
        SELECT TRUNC(CREATE_DATE, 'iw') as WEEK_FIRST,
        CREATE_DATE
        FROM MEMBER_VIEW
     </select>

</mapper>
